{
  "name": "universal-proxy-server",
  "version": "1.0.1",
  "description": "Universal HTTP proxy server with CORS support",
  "main": "dist/proxy.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/proxy.ts",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "start": "node dist/proxy.js",
    "test": "jest --coverage",
    "prepublish": "pnpm build"
  },
  "keywords": [],
  "author": "LV",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}